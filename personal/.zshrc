# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"

# Enviroment Variables
XDG_CONFIG_HOME="$HOME/.config"

# Exports
export EDITOR=nvim
export XDG_CONFIG_HOME
export TERM=xterm-256color
export COLORTERM=truecolor


# Aliases
alias hist="history | less"
alias gg="git log --graph --oneline --all"
alias vim="nvim"
alias vba="visualboyadvance-m"
alias dolphin="dolphin-emu"
alias gl="git log --graph --decorate --pretty=oneline --abbrev-commit"
alias projects="/home/jack/Documents/Projects/"
alias botting="/home/jack/Documents/Botting/"
alias akariWorkUI="/home/jack/Documents/Botting/Akari/Akari-UI/ && tmux"
alias ssh_robin_track="ssh -i '~/Documents/Projects/robin_track/research/robin_track_ec2.pem' ubuntu@ec2-3-131-208-10.us-east-2.compute.amazonaws.com"
alias huskids="sudo netctl start wlp3s0-Huskids"
alias parents="sudo netctl start wlp3s0-FiOS-ZOG2T"
alias cynthia="sudo netctl start wlp3s0-TP-Link_23E1"
alias studio="sudo netctl start wlp3s0-QianChenCo"
alias phone="sudo netctl start 'wlp3s0-OnePlus 6T-678d'"
alias airbnb="sudo netctl start wlp3s0-MySpectrumWiFi50-5G"

# Set resolution if DP1 plugged in
~/.scripts/set_monitor.py

